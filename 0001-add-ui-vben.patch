+    pnpm install --frozen-lockfile && \
+    pnpm build:docker
+# node
+RUN echo "        "
+# nginx 
+FROM nginx:1.23.3-alpine as production-stage
+COPY --from=build-stage /app/dist /usr/share/nginx/html/dist
+COPY --from=build-stage /app/nginx.conf /etc/nginx/nginx.conf
+EXPOSE 80
+## /usr/share/nginx/html/dist/assets/index.js /usr/share/nginx/html/dist/_app.config.js"$vg_base_url"VG_BASE_URL,$vg_sub_domain VG_SUB_DOMAIN$vg_default_userVG_DEFAULT_USER$vg_default_passwordVG_DEFAULT_PASSWORD nginx
