+  import { BasicTable, TableActionType } from '@/components/Table'
+  import { getBasicColumns, getBasicShortColumns } from './tableData'
+  import { useMessage } from '@/hooks/web/useMessage'
+  import { demoListApi } from '@/api/demo/table'
