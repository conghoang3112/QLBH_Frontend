+    background-color: @app-content-background;
+
+    .disabeld {
+      color: @disabled-color;
+    }
+
+    &__icon {
+      display: inline-block;
+      margin-right: 10px;
+      padding: 2px 4px;
+
+      &:hover {
+        color: @primary-color;
+      }
+    }
+  }
+</style>
diff --git a/src/components/FlowChart/src/adpterForTurbo.ts b/src/components/FlowChart/src/adpterForTurbo.ts
