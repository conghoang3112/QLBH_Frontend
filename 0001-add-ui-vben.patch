+        (childRenderOpts: VxeGlobalRendererHandles.RenderDefaultOptions) =>
+          buttonDefaultRender(childRenderOpts, params)[0],
+      )
+    }
+    return []
+  }
+}
+
+function createButtonItemRender() {
