+        <TabPane
+          v-for="item in tabsFormSchema"
+          :key="item.key"
+          v-bind="omit(item, ['Form', 'key'])"
+        >
+          <BasicForm @register="item.Form[0]" />
+        </TabPane>
+      </Tabs>
+    </CollapseContainer>
+  </PageWrapper>
