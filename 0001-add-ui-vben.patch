+        transition: all 0.3s;
+        border: 1px solid @border-color;
+        border-radius: 3px;
+        line-height: 20px;
+        cursor: move;
+
+        &:hover {
+          position: relative;
+          border: 1px solid @primary-color;
+          // z-index: 1;
+          box-shadow: 0 2px 6px @primary-color;
