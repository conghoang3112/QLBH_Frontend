+<template>
+  <div>
+    <div v-if="['Grid'].includes(formConfig.currentItem!.component)">
+      <div v-for="(item, index) of formConfig.currentItem!['columns']" :key="index">
+        <div class="options-box">
+          <Input v-model:value="item.span" class="options-value" />
