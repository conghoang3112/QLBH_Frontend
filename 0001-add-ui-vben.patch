+  const wrapRef = ref<HTMLDivElement | null>(null)
+  const { toPromise } = useScript({ src: BAI_DU_MAP_URL })
+
+  async function initMap() {
+    await toPromise()
+    await nextTick()
+    const wrapEl = unref(wrapRef)
+    if (!wrapEl) return
