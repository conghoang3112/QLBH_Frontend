+export const deptMap = (() => {
+  const pDept = ['', '', '']
+  const cDept = ['', '', '', '']
+
+  return pDept.reduce((map, p, pIdx) => {
+    map[pIdx] = p
+
+    cDept.forEach((c, cIndex) => (map[`${pIdx}-${cIndex}`] = `${p}-${c}`))
