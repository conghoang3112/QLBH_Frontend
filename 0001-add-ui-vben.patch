+  const handleClick = async () => {
+    await testRetry()
+  }
+
+  function apiError() {
+    return new Promise<void>((resolve, reject) => {
+      setTimeout(() => {
+        reject(`TimeError: ${Date.now()}`)
+      }, 1300)
+    })
+  }
+
+  // PS >> useRequest API(: axios)
+
+  //  API 
