+export function limit(fn: any, timespan: number) {
+  let pending = false
+
+  return (...args: any[]) => {
+    if (pending) return
+    pending = true
+    fn(...args)
+    setTimeout(() => {
+      pending = false
+    }, timespan)
+  }
+}
