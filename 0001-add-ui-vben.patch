+
+export function isDef<T = unknown>(val?: T): val is T {
+  return typeof val !== 'undefined'
+}
+
+// TODO 此处 isObject 存在歧义
+export function isObject(val: any): val is Record<any, any> {
+  return val !== null && is(val, 'Object')
