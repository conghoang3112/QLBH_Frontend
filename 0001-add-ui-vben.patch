+    refundTimeTableData,
+  } from './data'
+
+  const [registerRefundTable] = useTable({
+    title: '退货商品',
+    dataSource: refundTableData,
+    columns: refundTableSchema,
+    pagination: false,
+    showIndexColumn: false,
+    scroll: { y: 300 },
+    showSummary: true,
+    summaryFunc: handleSummary,
+  })
